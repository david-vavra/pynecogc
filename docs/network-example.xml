<?xml version="1.0" encoding="utf-8"?>
<network>
<!-- DEVICES -->
<device>
	<fqdn>sw14.company.com</fqdn>
	<snmp version="2c" id="ip-mac-port" privilege="read-only">public</snmp>
	<snmp version="2c" id="backup" privilege="read-write">private</snmp>
	<filename>sw14.company.com</filename>
	<vendor>cisco</vendor>
	<type>WS-C2960-48TC-L</type>
	<l2>true</l2>
	<l3>false</l3>
	<ipv6>false</ipv6>
	<vlanbackup>false</vlanbackup>
	<group_member>UVT</group_member>
	<dhcp_snooping>
	<trusted_interface>Gi0/1</trusted_interface>
	<trusted_interface>Gi0/2</trusted_interface>
	</dhcp_snooping>
</device>

<!-- ACLS  -->
<aclv4 id="uvt_vty_snmp">
		<name>VTY</name>
		<name id="cisco">71</name>
		<type id="cisco">extended</type>
		<rule seq="10">
			<protocol state="established">tcp</protocol>
			<source mask="26" port="22">174.152.1.0</source>
			<destination mask="0">0.0.0.0</destination>
			<action log="True">permit</action>				
		</rule>
		<rule seq="20">
			<comment>deny any other traffic</comment>
			<source mask="0">0.0.0.0</source>
			<destination mask="0">0.0.0.0</destination>
			<action>deny</action>
		</rule>
</aclv4>
<aclv4 id="ntp">
            <type id="cisco">standard</type>
			<name>77</name>
    		<rule seq="10">
				<source mask="32">174.152.7.38</source>
			    <action>permit</action>
			</rule>
			<rule seq="20">
				<source mask="32">174.152.7.17</source>
				<action>permit</action>
			</rule>
</aclv4>

<!-- GROUPS -->
<groups>
	<group id="DEFAULT">
        <aaa>
            <aaa_group id="MGMThost" type="tacacs"></aaa_group>
            <aaa_host id="MGMThost" group="THINGOL" type="tacacs">174.152.7.17</aaa_host>
            <aaa_method_list name="console">
		        <type id="cisco">login</type>
		        <type id="comware">login</type>
		        <method>group MGMThost</method>
		        <method>line</method>
            </aaa_method_list>
        </aaa>
        <ntp acl_id="ntp">
			<host id="1">10.0.0.1</host>
			<host id="2">10.0.0.2</host>
		</ntp>
		<dns_host id="1">174.152.6.10</dns_host>
		<vty>
			<protocol version="2" timeout="120" retries="3">ssh</protocol>
			<acl_id>uvt_vty_snmp</acl_id>
		</vty>
        <syslog severity="warning" facility="local7">
            <syslog_server id="mgmthost">174.152.7.17</syslog_server>
        </syslog>
		<vtp>
			<version>2</version>
			<domain>UVT</domain>
			<password>vtp-uvt</password>
			<!-- role jednotlivych zarizeni ? -->
		</vtp>
		<snmp>
			<trap_host id="1" version="2c" tags="all,auth" community="UVT_trap">mgmthost.company.com</trap_host>
            <trap_host id="1" version="3" tags="all,auth" user="vavra" authLevel="priv">mgmthost.company.com</trap_host>
			<trap tags="auth">aaa_server</trap>
			<trap tags="all">authenticate-fail</trap>
			<community version="2c" id="backupp" privilege="read-write" acl_id="uvt_vty_snmp">2a10ha</community>
			<community version="2c" id="ipp-mac-port" privilege="read-only" acl_id="uvt_vty_snmp">ics-1nterna1</community>
            <view id="testview">
                <tree type="included">internet</tree>
                <tree type="excluded">iso</tree>
            </view>
            <!--<group name="name_or_community_in_v2" secModel=[1,2c,3]  if secModel==3 authLevel=[noAuth,auth,priv]>-->
            <group id="groupName" secModel="3" authLevel="priv" acl="aclId">
                <!-- type=[read,write] -->
                <_view type="read">testView</_view>
            </group>
            <!--version=[1,2c,3], -->
            <user group="groupName" version="2c" acl="aclId">
                <username>test</username>
            </user>
            <user group="groupName" version="3" acl="aclId">
                <username>vavra</username>
                <!-- auth=[md5,sha]-->
                <auth encrypted="False" type="md5">authString</auth>
                <!-- type={des | 3des | aes {128 | 192 | 256}} -->
                <priv encrypted="False" type="aes256">privString</priv>
            </user>
		</snmp>
		<vty>
			<vty_vlan>50</vty_vlan>
			<vty_gw>192.168.50.1</vty_gw>
		</vty>
		<dhcp_snooping>
			<vlan_range>1-4094</vlan_range>
		</dhcp_snooping>
		<arp_inspection>
			<vlan_range>1-4094</vlan_range>
		</arp_inspection>
        <ip_source_guard>
            <vlan_range>1-4094</vlan_range>
        </ip_source_guard>
		<errdisable>TODO</errdisable>
        <hsrp_instance id='50'>
			<interface>Vlan110</interface>
			<timers>
				<hello>4</hello>
				<dead>16</dead>
			</timers>
		</hsrp_instance>
        <urpf type="strict">DEFAULT</urpf>
		<urpf type='strict'>Vlan70</urpf>
		<urpf type='loose'>TenGigabitEthernet1/1</urpf>
	</group>
</groups>
</network>
